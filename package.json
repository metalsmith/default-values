{
  "name": "metalsmith-default-values",
  "version": "2.1.0",
  "description": "A metalsmith plugin for setting default values for frontmatter metadata.",
  "engines": {
    "node": ">=4.x"
  },
  "dependencies": {
    "debug": "^4.0.0",
    "joi": "^12.0.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "multimatch": "^2.1.0"
  },
  "devDependencies": {
    "eslint": "^4.4.0",
    "eslint-config-prettier": "^3.0.0",
    "eslint-plugin-prettier": "^2.5.0",
    "nodemon": "^1.11.0",
    "prettier": "^1.10.2",
    "tap": "^12.0.1"
  },
  "greenkeeper": {
    "ignore": [
      "joi",
      "eslint"
    ]
  },
  "main": "lib/index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "tap test -J",
    "lint": "eslint --cache --fix .",
    "dev": "nodemon --exec 'npm test'",
    "preversion": "npm test",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/woodyrew/metalsmith-default-values.git"
  },
  "keywords": [
    "Metalsmith",
    "Plugin",
    "default"
  ],
  "author": {
    "name": "Woody Goodricke",
    "email": "github@andrewgoodricke.com",
    "url": "http://andrewgoodricke.com"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/woodyrew/metalsmith-default-values/issues"
  },
  "homepage": "https://github.com/woodyrew/metalsmith-default-values#readme"
}
