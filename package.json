{
  "name": "@metalsmith/default-values",
  "version": "3.3.0",
  "description": "A metalsmith plugin for setting default values to file metadata.",
  "keywords": [
    "default-values",
    "metalsmith-plugin",
    "metalsmith",
    "static-site"
  ],
  "homepage": "https://github.com/metalsmith/default-values#readme",
  "bugs": {
    "url": "https://github.com/metalsmith/default-values/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metalsmith/default-values.git"
  },
  "license": "GPL-3.0",
  "author": "Woody Goodricke <github@andrewgoodricke.com>",
  "maintainers": [
    "Kevin Van Lierde (https://webketje.com)"
  ],
  "source": "src/index.js",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "exports": {
    "import": "./lib/index.js",
    "require": "./lib/index.cjs"
  },
  "type": "module",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "lib",
    "CHANGELOG.md"
  ],
  "scripts": {
    "changelog": "auto-changelog -u --commit-limit false --ignore-commit-pattern '^((dev|chore|ci):|Release)'",
    "format": "prettier --write \"**/*.{yml,md,js,json}\"",
    "format:check": "prettier --list-different \"**/*.{yml,md,js,json}\"",
    "lint": "eslint --fix .",
    "lint:check": "eslint --fix-dry-run .",
    "coverage": "nyc report --reporter=text-lcov > ./coverage.info",
    "release": "release-it .",
    "build": "microbundle --target node --no-sourcemap -f cjs,esm --generateTypes=false",
    "test": "nyc mocha",
    "pretest": "npm run build"
  },
  "dependencies": {
    "dlv": "^1.1.3",
    "dset": "^3.1.2"
  },
  "devDependencies": {
    "auto-changelog": "^2.4.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "metalsmith": "^2.5.1",
    "microbundle": "^0.15.1",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "prettier": "^2.8.4",
    "release-it": "^15.6.0"
  },
  "peerDependencies": {
    "metalsmith": "^2.5.0"
  },
  "engines": {
    "node": ">=12.17.0 <13.0.0-0||>=13.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
